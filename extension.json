{
	"name": "FlickrAPI",
	"version": "2.0.2",
	"license-name": "GPL-2.0-or-later",
	"author": [
		"Ike Hecht",
		"Sam Wilson"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:FlickrAPI",
	"descriptionmsg": "flickrapi-desc",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"MessagesDirs": {
		"FlickrAPI": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"FlickrAPIHooks": "includes/FlickrAPIHooks.php",
		"FlickrAPIUtils": "includes/FlickrAPIUtils.php",
		"FlickrAPICache": "includes/FlickrAPICache.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "FlickrAPIHooks::onParserFirstCallInit"
	},
	"config": {
		"FlickrAPIKey": "",
		"FlickrAPISecret": "",
		"FlickrAPIDefaults": {
			"type": "frameless",
			"location": "right",
			"size": "-"
		}
	},
	"load_composer_autoloader": true,
	"manifest_version": 1
}
